using test := cpp("WideLibrary/test.h");
using Range := Standard.Range;
using Algorithm := Standard.Algorithm;

Main() {
    x := std.vector!(int8)();
    Range.Delimited(1, 11) | Algorithm.Copy(Range.BackInserter(x));
    Range.Delimited(x.begin(), x.end())
        | Algorithm.Map(function(it) { return *it; })
        | Algorithm.Map(function(val) { return Range.Delimited(1, val + 1); })
        | Algorithm.Flatten()
        | Algorithm.Copy(Range.StreamInserter(std.cout));
}