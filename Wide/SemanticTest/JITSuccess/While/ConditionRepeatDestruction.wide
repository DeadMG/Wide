type testty {
    result := int64.pointer;
    ~type() {
         (*result)++;
    }
    f() {
        return true;
    }
    type(int64.pointer arg)
        : result(arg) {}
}
Main() {
    des := 0;
    counter := 0;
    while(testty(&des).f() & counter ~= 5) {
        ++counter;
    }
    return des == counter + 1;
}