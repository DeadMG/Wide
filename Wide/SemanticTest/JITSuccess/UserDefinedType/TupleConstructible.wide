type inner {
    x := int64;
    y := int64;
    type(int64 blah)
        : x(blah) : y(blah) {}
    valid(int64 arg) {
        return x == arg & y == arg;
    }
}
type outer {
    first := inner;
    second := inner;
    valid(int64 f, int64 s) {
        return first.valid(f) & second.valid(s);
    }
}
Main() {
    var := outer({1, 2});
    return var.valid(1, 2);
}