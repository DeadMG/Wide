type t {
    value : bool.pointer;
    type(val : bool.lvalue) : value := &val {}
    type(copy : t.lvalue) : value := copy.value {
        *value = true;
    }
    type(move : t.rvalue) : value := move.value {}
}
Main() {
    result := false;
    var := { true, t(result) };
    copy := var;
    left, right := copy;
    return left & result;
}