type t {
    value := bool->pointer;
    type(bool->lvalue val) : value(&val) {}
    type(t->lvalue copy) : value(copy.value) {
        *value = true;
    }
    type(t->rvalue move) : value(move.value) {}
}
Main() {
    result := false;
    var := { true, t(result) };
    copy := var;
    left, right := copy;
    return left & result;
}