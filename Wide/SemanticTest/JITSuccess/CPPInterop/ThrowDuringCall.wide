using header := cpp.literal("struct T { T(int* ptr) : ptr(ptr) {} int* ptr; ~T() { (*ptr)++; } }; void f(T t) { throw true; }");
Main() {
    result := int32(0);
    {
        try { 
            header.f(header.T(&result));
        } catch(arg := bool.lvalue) {
            return result == 1 & arg;
        }
    }
    return false;
}