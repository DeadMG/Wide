type b1 {
    type() {}
    dynamic f() { return false; }
}
type b2 {
    type() {}
    dynamic g() { return false; }
}
type der : b1 : b2 {
    type() {}
    f() { return true; }
    g() { return true; }
}
f(arg1 := b1.lvalue, arg2 := b2.lvalue) {
    return arg1.f() & arg2.g();
}
Main() {
    var := der();
    return f(var, var);
}