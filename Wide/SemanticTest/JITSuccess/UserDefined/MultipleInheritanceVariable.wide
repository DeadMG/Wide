type b1 {
    type() {}
    dynamic f() { return false; }
}
type b2 {
    type() {}
    dynamic g() { return false; }
}
type der : b1 : b2 {
    b := bool;
    type(x := bool) : b(x) {}
    f() { return b; }
    g() { return b; }
}
f(arg1 := b1.lvalue, arg2 := b2.lvalue) {
    return arg1.f() & arg2.g();
}
Main() {
    var := der(true);
    return f(var, var);
}