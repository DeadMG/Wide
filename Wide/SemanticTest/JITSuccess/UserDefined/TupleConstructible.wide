type inner {
    x := int64;
    y := int64;
    type(blah := int64)
        : x := blah : y := blah {}
    valid(arg := int64) {
        return x == arg & y == arg;
    }
}
type outer {
    first := inner;
    second := inner;
    valid(f := int64, s := int64) {
        return first.valid(f) & second.valid(s);
    }
}
Main() {
    var := outer({1, 2});
    return var.valid(1, 2);
}